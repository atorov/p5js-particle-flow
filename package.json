{
    "name": "p5js-particle-flow",
    "version": "1.0.0",
    "scripts": {
        "build": "npm run clean && mkdir -p ./build && cp ./*.html ./build/ && cp favicon.ico ./build/ && cp -r ./src ./build/ && cp -r ./img ./build",
        "clean": "rm -rf ./build/",
        "serve": "npm run build && serve ./build",
        "start": "reload -w ./ -b -v"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/atorov/p5js-particle-flow.git"
    },
    "author": "atorov",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/atorov/p5js-particle-flow"
    },
    "homepage": "https://github.com/atorov/p5js-particle-flow#readme",
    "devDependencies": {
        "eslint": "^6.1.0",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-plugin-import": "^2.18.2",
        "reload": "^3.0.1",
        "serve": "^11.1.0"
    }
}
